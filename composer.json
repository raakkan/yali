{
    "name": "raakkan/yali",
    "description": "Another laravel admin panel",
    "type": "stable",
    "license": "mit",
    "autoload": {
        "psr-4": {
            "Raakkan\\Yali\\": "src/",
            "Raakkan\\Yali\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Raakkan\\Yali\\Database\\Factories\\": "database/factories/",
            "Raakkan\\Yali\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "authors": [
        {
            "name": "raakkan",
            "email": "solakkathir@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "wikimedia/composer-merge-plugin": "^2.1",
        "illuminate/support": "^11.7",
        "illuminate/console": "^11.7",
        "intervention/image": "^3.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raakkan\\Yali\\Providers\\TranslationServiceProvider",
                "Raakkan\\Yali\\Providers\\YaliServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}
