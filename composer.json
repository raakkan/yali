{
    "name": "raakkan/yali",
    "description": "Another laravel admin panel",
    "type": "stable",
    "license": "mit",
    "autoload": {
        "psr-4": {
            "Raakkan\\Yali\\": "src/"
        },
        "files": [
            "src/helpers/plugin-helpers.php"
        ]
    },
    "authors": [
        {
            "name": "raakkan",
            "email": "solakkathir@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "livewire/livewire": "^3.4",
        "wikimedia/composer-merge-plugin": "^2.1",
        "illuminate/support": "^11.7",
        "illuminate/console": "^11.7",
        "blade-ui-kit/blade-icons": "^1.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raakkan\\Yali\\Providers\\YaliServiceProvider",
                "Raakkan\\Yali\\Providers\\PluginServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}
