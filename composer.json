{
    "name": "raakkan/yali",
    "description": "Another laravel admin panel",
    "type": "stable",
    "license": "mit",
    "autoload": {
        "psr-4": {
            "Raakkan\\Yali\\": "src/"
        },
        "files": [
            "src/helpers/plugin-helpers.php"
        ]
    },
    "authors": [
        {
            "name": "raakkan",
            "email": "solakkathir@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^11.6",
        "illuminate/console": "^11.6",
        "wikimedia/composer-merge-plugin": "^2.0@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raakkan\\Yali\\Providers\\YaliServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}